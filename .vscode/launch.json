{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",

			"name": "Debug Main",
			"env": {
				"ENV_PATH": "${workspaceFolder}/.env",
				"BABEL_ENV": "debug"
			},
			"runtimeExecutable": "${workspaceFolder}/node_modules/.bin/babel-node",
			"program": "${workspaceFolder}/bin/www",
			"outputCapture": "std",
			"sourceMaps": true,
			"console": "integratedTerminal"
		}
	]
}



